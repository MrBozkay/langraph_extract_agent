# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_NAME=scraped-content
MINIO_SECURE=false

# LLM Configuration
# For Gemini API (initial setup)
GOOGLE_API_KEY=your_gemini_api_key_here
LANGEXTRACT_MODEL=gemini-2.0-flash-exp

# For future Ollama support (local inference)
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_MODEL=gpt-oss:20b

# Extraction Settings
EXTRACTION_BATCH_SIZE=10
EXTRACTION_RETRY_COUNT=3
EXTRACTION_RETRY_DELAY=2
EXTRACTION_TIMEOUT=30
EXTRACTION_MAX_WORKERS=5

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=20
RATE_LIMIT_DELAY_BETWEEN_REQUESTS=3

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/extraction.log
